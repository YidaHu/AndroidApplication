<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
          package="com.huyd.alertdialogdemo">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>

                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
        <activity android:name="com.huyd.OrdinaryDialogActivity">
        </activity>
        <activity android:name=".OrdinaryDialogActivity">
        </activity>
        <activity android:name=".InputAlertDialogActivity">
        </activity>
        <activity android:name=".LoginAlertDialogActivity">
        </activity>
        <activity android:name=".WelcomeLoadingActivity">
        </activity>
        <activity android:name=".SelfActivity">
        </activity>
    </application>

</manifest>